{
  "expo": {
    "name": "Lista de tarefas do Tiago",
    "slug": "list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B202D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1B202D"
      },
      "package": "com.tiagobrandaoo.list"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
